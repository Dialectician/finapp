.fadeIn
  &-enter-active
  &-leave-active
    opacity 1
    transition all 200ms ease

  &-enter
  &-leave-to
    opacity 0


.animModal
  &-enter-active
  &-leave-active
    opacity 1
    transform translate(0, 0)
    transition all 300ms cubic-bezier(0.17, 0.04, 0.03, 0.94)

  &-enter
  &-leave-to
    opacity 0
    transform translate(0, 10px)


